.swiper_box swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_box swiper image {
  width: 100%;
}
.cate_box {
  display: flex;
}
.cate_box navigator {
  flex: 1;
  padding: 20px;
}
.cate_box navigator image {
  width: 100%;
}
.floor_box .floor_title {
  padding: 10rpx 5px;
}
.floor_box .floor_title image {
  width: 100%;
}
.floor_box .floor_list {
  padding: 0 5rpx;
}
.floor_box .floor_list::after {
  content: "";
  display: block;
  clear: both;
}
.floor_box .floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_box .floor_list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_box .floor_list navigator:nth-of-type(2),
.floor_box .floor_list navigator:nth-of-type(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_box .floor_list navigator image {
  width: 100%;
  height: 100%;
}
